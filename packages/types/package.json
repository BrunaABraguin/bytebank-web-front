{
  "name": "@workspace/types",
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "check-types": "tsc --noEmit"
  },
  "exports": {
    "./account": {
      "types": "./src/account.ts",
      "default": "./dist/account.js"
    },
    "./form": {
      "types": "./src/form.ts",
      "default": "./dist/form.js"
    },
    "./option": {
      "types": "./src/option.ts",
      "default": "./dist/option.js"
    },
    "./transaction": {
      "types": "./src/transaction.ts",
      "default": "./dist/transaction.js"
    },
    "./user": {
      "types": "./src/user.ts",
      "default": "./dist/user.js"
    }
  },
  "devDependencies": {
    "@workspace/typescript-config": "*",
    "typescript": "5.8.2"
  }
}